	<queryString>
		<![CDATA[SELECT
cb.partner_name,
mp.product_code,
mp.product_name,
mp.spec,
mp.od,
mp.thickness,
mp.length,
sum(cfl.quantity),
max( CASE WHEN cf.periode= $P{REPORT_PERIOD_1} THEN cfl.quantity ELSE 0 end ) AS bulansatu,
(SELECT COALESCE(sum( col.order_quantity ),0)
FROM c_order co, c_order_line col
WHERE co.c_order_id = col.c_order_id
AND col.m_product_id = mp.m_product_id
AND month( co.order_date ) = $P{REPORT_MONTH_1}
AND year( co.order_date ) = $P{REPORT_YEAR_1}
AND co.c_bpartner_id =$P{REPORT_PARTNER}) AS sobulan1,

(SELECT COALESCE(sum( col.order_quantity ),0)
FROM c_order co, c_order_line col
WHERE co.c_order_id = col.c_order_id
AND col.m_product_id = mp.m_product_id
AND month( co.order_date ) = $P{REPORT_MONTH_2}
AND year( co.order_date ) = $P{REPORT_YEAR_2}
AND co.c_bpartner_id =$P{REPORT_PARTNER}) AS sobulan2,

(SELECT COALESCE(sum( col.order_quantity ),0)
FROM c_order co, c_order_line col
WHERE co.c_order_id = col.c_order_id
AND col.m_product_id = mp.m_product_id
AND month( co.order_date ) = $P{REPORT_MONTH_3}
AND year( co.order_date ) = $P{REPORT_YEAR_3}
AND co.c_bpartner_id =$P{REPORT_PARTNER}) AS sobulan3,

max( CASE WHEN cf.periode= $P{REPORT_PERIOD_2} THEN cfl.quantity ELSE 0 end ) AS bulandua,
max( CASE WHEN cf.periode= $P{REPORT_PERIOD_3} THEN cfl.quantity ELSE 0 end ) AS bulantiga

FROM c_forecast_line cfl

JOIN m_product mp USING (m_product_id)
JOIN c_forecast cf USING (c_forecast_id)
JOIN c_bpartner cb ON (cf.c_bpartner_id = cb.c_bpartner_id)

WHERE cf.c_bpartner_id= $P{REPORT_PARTNER}
GROUP BY mp.product_code]]>
	</queryString>
