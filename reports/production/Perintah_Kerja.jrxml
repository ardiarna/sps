	<queryString>
		<![CDATA[SELECT c_spk.document_no spk, spk_date, c_wo.document_no wo, m_machine.machine_name, c_spk_line.quantity, fg.spec, fg.od, fg.thickness,
fg.length, mat.od od_mat, mat.thickness thickness_mat, mat.length length_mat, (c_spk_line.quantity * m_material_requirement.divqty) quantity_mat
FROM c_spk JOIN c_wo USING (c_wo_id) JOIN m_machine USING (m_machine_id) JOIN c_spk_line USING (c_spk_id)
JOIN c_wo_line USING (c_wo_line_id)
JOIN m_product fg ON (c_wo_line.m_product_id=fg.m_product_id)
LEFT JOIN m_material_requirement ON (c_wo_line.m_product_id=m_material_requirement.m_product_fg)
LEFT JOIN m_product mat ON (m_material_requirement.m_product_material=mat.m_product_id)
JOIN c_production_plan USING (c_production_plan_id) WHERE spk_date BETWEEN $P{REPORT_DATE_START} AND $P{REPORT_DATE_END} AND c_production_plan.app_org_id = $P{REPORT_ORG}
ORDER BY spk_date, spk]]>
	</queryString>
