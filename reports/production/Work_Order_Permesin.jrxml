	<queryString>
		<![CDATA[SELECT c_wo.document_no, c_wo.wo_date, m_machine.machine_name, partner_name, m_product.od, c_order.reference_no, c_order.remark, m_product.thickness, m_product.length, mate.od ode_mt, mate.thickness thickness_mt, mate.length length_mt, SUM(c_wo_line.quantity),
SUM( CASE WHEN c_wo_line.working_date = $P{DAY_1} THEN c_wo_line.quantity ELSE 0 END) as day1,
SUM( CASE WHEN c_wo_line.working_date = $P{DAY_2} THEN c_wo_line.quantity ELSE 0 END) as day2,
SUM( CASE WHEN c_wo_line.working_date = $P{DAY_3} THEN c_wo_line.quantity ELSE 0 END) as day3,
SUM( CASE WHEN c_wo_line.working_date = $P{DAY_4} THEN c_wo_line.quantity ELSE 0 END) as day4,
SUM( CASE WHEN c_wo_line.working_date = $P{DAY_5} THEN c_wo_line.quantity ELSE 0 END) as day5,
SUM( CASE WHEN c_wo_line.working_date = $P{DAY_6} THEN c_wo_line.quantity ELSE 0 END) as day6
FROM c_wo_line
JOIN c_production_plan ON (c_wo_line.c_production_plan_id=c_production_plan.c_production_plan_id)
JOIN c_delivery_plan ON (c_production_plan.plan_ref = c_delivery_plan.c_delivery_plan_id)
JOIN c_order_line ON (c_delivery_plan.order_line_ref = c_order_line.c_order_line_id)
JOIN c_order USING (c_order_id)
LEFT JOIN c_bpartner ON (c_order.c_bpartner_id = c_bpartner.c_bpartner_id)
JOIN m_product ON ( c_wo_line.m_product_id=m_product.m_product_id)
LEFT JOIN m_material_requirement ON(c_wo_line.m_product_id=m_material_requirement.m_product_fg)
LEFT JOIN m_product mate ON(m_material_requirement.m_product_material=mate.m_product_id)
LEFT JOIN m_machine USING (m_machine_id)
JOIN c_wo USING (c_wo_id)
WHERE c_wo.c_wo_id = $P{REPORT_WO} AND c_wo_line.m_machine_id = $P{MESIN}
GROUP BY m_product.product_code
ORDER BY m_machine.machine_name]]>
	</queryString>
